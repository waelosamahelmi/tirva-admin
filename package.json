  "name": "tirva-admin",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:mobile": "concurrently \"npm run dev:frontend\" \"npm run dev:mobile-backend\"",
    "dev:mobile-backend": "cross-env NODE_ENV=development tsx server/mobile-server.ts",
    "build": "vite build && esbuild server/mobile-server.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:frontend": "vite build",
    "build:netlify": "vite build && npm run build:functions",
    "build:functions": "esbuild netlify/functions/api.ts --platform=node --packages=external --bundle --format=esm --outdir=netlify/functions --allow-overwrite",
    "build:mobile": "vite build && esbuild server/mobile-server.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/mobile-server.js",
    "start:mobile": "cross-env NODE_ENV=production node dist/mobile-server.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "test-supabase": "tsx test-supabase-connection.ts",
    "import-existing-data": "tsx import-existing-data.ts",
    "export-data": "tsx export-data.ts",
    "import-to-supabase": "tsx import-to-supabase.ts",
    "android:build": "npx vite build --mode android",
    "android:dev": "npx vite --mode android",
    "android:sync": "npx cap sync android",
    "android:run": "npx cap run android",
    "android:open": "npx cap open android",
    "android:build-apk": "npm run build:mobile && npx cap sync android && npx cap run android --no-sync",
    "android:release": "npm run build:mobile && npx cap sync android && cd android && ./gradlew assembleRelease"
  },
